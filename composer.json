{
  "name": "spryker/project",
  "description": "Project",
  "license": "MIT",
  "require": {
    "spryker/spryker": "dev-bugfix/php7-comp",

    "php": ">=5.6.0",
    "braintree/braintree_php": "~3.15.0",
    "guzzlehttp/guzzle": "~6.2.1",
    "league/csv": "~8.1.0",
    "mandrill/mandrill": "~1.0.55",
    "monolog/monolog": "~1.21.0",
    "nesbot/carbon": "~1.13.0",
    "pimple/pimple": "~1.1.1",
    "predis/predis": "~1.0.3",
    "propel/propel": "2.0.0-alpha5",
    "psr/log": "~1.0.0",
    "ruflin/elastica": "~3.2.0",
    "silex/silex": "~1.3.5",
    "symfony-cmf/routing": "~1.3.0",
    "symfony/console": "~2.6.13",
    "symfony/debug": "~2.6.13",
    "symfony/filesystem": "~2.6.13",
    "symfony/finder": "~2.6.13",
    "symfony/form": "~2.6.13",
    "symfony/http-foundation": "~2.8.0",
    "symfony/http-kernel": "~2.6.13",
    "symfony/intl": "~2.8.8",
    "symfony/options-resolver": "~2.6.13",
    "symfony/process": "~2.6.13",
    "symfony/property-access": "~2.6.13",
    "symfony/routing": "~2.6.13",
    "symfony/security": "~2.6.13",
    "symfony/serializer": "~2.6.13",
    "symfony/translation": "~2.6.13",
    "symfony/twig-bridge": "~2.6.13",
    "symfony/validator": "~2.6.13",
    "symfony/var-dumper": "~2.6.13",
    "symfony/yaml": "~2.6.13",
    "twig/twig": "~1.20.0",
    "zendframework/zend-code": "~2.6.2",
    "zendframework/zend-config": "~2.5.1",
    "zendframework/zend-eventmanager": "~2.6.2",
    "zendframework/zend-filter": "~2.5.1",
    "zendframework/zend-http": "~2.5.3",
    "zendframework/zend-servicemanager": "~2.7.4"
  },
  "require-dev": {
    "codeception/codeception": "~2.1.10",
    "codeception/c3": "2.0.0",
    "pdepend/pdepend": "~2.2.2",
    "phploc/phploc": "~3.0.1",
    "phpmd/phpmd": "^2.4",
    "sebastian/phpcpd": "~2.0.2",
    "sensiolabs/security-checker": "~3.0.2",
    "sstalle/php7cc":"1.0.0",
    "silex/web-profiler": "~1.0.8",
    "site5/phantoman": "^1.1",
    "sllh/composer-versions-check": "~2.0.2",
    "spryker/code-sniffer": "*",
    "jakoch/phantomjs-installer": "^2.1",
    "php-mock/php-mock": "^1.0",
    "spryker/architecture-sniffer": "*"
  },
  "autoload": {
    "psr-0": {
      "": "src",
      "Unit": "tests",
      "YvesUnit": "tests",
      "Functional": "tests",
      "Acceptance": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "use-include-path": true,
    "process-timeout": 600
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:spryker/spryker.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/architecture-sniffer.git"
    }
  ],
  "scripts": {
    "post-install-cmd": [
      "PhantomInstaller\\Installer::installPhantomJS"
    ],
    "post-update-cmd": [
      "PhantomInstaller\\Installer::installPhantomJS"
    ]
  }
}
