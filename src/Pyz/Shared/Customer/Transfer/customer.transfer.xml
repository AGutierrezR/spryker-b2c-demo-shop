<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">
  
    <transfer name="Customer">
        <property name="phone" type="string" />
    </transfer>

    <transfer name="CustomerOverviewRequest">
        <property name="customer" type="Customer" />
        <property name="orderList" type="OrderList" />
    </transfer>

    <transfer name="CustomerOverviewResponse">
        <property name="orderList" type="OrderList" />
        <property name="isSubscribed" type="bool" />
    </transfer>

</transfers>

