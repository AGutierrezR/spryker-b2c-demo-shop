{% extends template('page-blank') %}

{% define data = {
    antelope: _view.antelope,
    store: _view.store,
} %}

{% block body %}
    Store: {{ data.store.name }}

    <br><br>
    
    {% if data.antelope is not null %}
        My antelope has the name {{ data.antelope.name }}
    {% else %}
        No antelope found :(
    {% endif %}
{% endblock %}
