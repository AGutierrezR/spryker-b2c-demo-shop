{% extends model('component') %}

{% define config = {
    name: 'video-replacer',
    tag: 'video-replacer'
} %}

{% define data = {
    url: required
} %}

{% block body %}

    <video src="" type="video/mp4" preload="auto" data-src="{{ data.url }}" autoplay="autoplay" loop="loop"></video>

{% endblock %}