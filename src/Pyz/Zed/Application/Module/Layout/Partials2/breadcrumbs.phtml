<ul class="breadcrumb">
<?php
foreach($this->pages as $page){
    /* @var $page Zend_Navigation_Page_Mvc */
    $classActive = $page->isActive()?'active':'';
    echo '<li class="'.$classActive.'">';
    echo '<a href="#'.$page->getHref().'">';
    echo $page->getLabel();
    echo '</a>';
    echo '</li>';
}
?>
</ul>