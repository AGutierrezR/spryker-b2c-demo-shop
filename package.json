{
  "private": true,
  "name": "spryker-suite",
  "version": "1.0.0",
  "description": "Spryker Suite project",
  "author": {
    "name": "Spryker",
    "email": "info@spryker.com",
    "url": "https://spryker.com/"
  },
  "scripts": {
    "yves": "node $npm_package_config_shopUi_modulePath/src/SprykerShop/Yves/ShopUi/Theme/default/builder/build dev",
    "yves:watch": "node $npm_package_config_shopUi_modulePath/src/SprykerShop/Yves/ShopUi/Theme/default/builder/build dev-watch",
    "yves:production": "node $npm_package_config_shopUi_modulePath/src/SprykerShop/Yves/ShopUi/Theme/default/builder/build prod",
    "zed": "node ./node_modules/@spryker/oryx-for-zed/build",
    "zed:watch": "node ./node_modules/@spryker/oryx-for-zed/build --dev"
  },
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.6.0"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "~1.1.0"
  },
  "devDependencies": {
    "@spryker/oryx": "^1.1.0",
    "@spryker/oryx-for-zed": "^1.1.1",
    "autoprefixer": "^8.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "~0.28.10",
    "fast-glob": "~2.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "~4.7.2",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-loader": "^2.1.3",
    "sass-loader": "~6.0.6",
    "ts-loader": "^4.2.0",
    "typescript": "~2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.5.0"
  },
  "config": {
    "shopUi": {
      "name": "yves_default",
      "theme": "default",
      "publicPath": "./public/Yves/assets",
      "modulePath": "./vendor/spryker/spryker-shop/Bundles/ShopUi"
    }
  }
}
