{
    "name": "saatchionline.com legacy-api",
    "apiVersion": "2013-04-18",
    "baseUrl": null,
    "operations": {
        "base": {
            "httpMethod": "POST",
            "responseType": "class",
            "parameters": {
                "User-Agent": {
                    "default": "Yves",
                    "location": "header"
                }
            }
        },
        "customer/legacy/login": {
            "class": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Request_CustomerLegacyLogin",
            "extends": "base",
            "uri": "/accounts/sign-in",
            "summary": "Logs in a customer",
            "responseClass": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Response_CustomerLegacyLogin",
            "parameters": {
                "X-Requested-With": {
                    "default": "XMLHttpRequest",
                    "location": "header"
                },
                "email": {
                    "location": "postField",
                    "type": "string",
                    "required": true
                },
                "password": {
                    "location": "postField",
                    "type": "string",
                    "required": true
                }
            }
        },
        "customer/legacy/register": {
            "class": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Request_CustomerLegacyRegister",
            "extends": "base",
            "uri": "/account/registration/register",
            "summary": "Registers a customer",
            "responseClass": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Response_CustomerLegacyRegister",
            "parameters": {
                "X-Requested-With": {
                    "default": "XMLHttpRequest",
                    "location": "header"
                },
                "user_type_id": {
                    "location": "postField",
                    "type": "string",
                    "required": true,
                    "default": "1"
                },
                "email": {
                    "location": "postField",
                    "type": "string",
                    "required": true
                },
                "first_name": {
                    "location": "postField",
                    "type": "string",
                    "required": false
                },
                "last_name": {
                    "location": "postField",
                    "type": "string",
                    "required": false
                },
                "password": {
                    "location": "postField",
                    "type": "string",
                    "required": true
                },
                "password_compare": {
                    "location": "postField",
                    "type": "string",
                    "required": true
                }
            }
        },
        "legacy/webservice/removesale": {
            "class": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Request_SalesLegacyItemCanceled",
            "extends": "base",
            "uri": "/ws1/sale/{id}",
            "httpMethod": "DELETE",
            "summary": "Send a notification to tell legacy about a sold item",
            "responseClass": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Response_SalesLegacyItemCanceled",
            "parameters": {
                "id": {
                    "location": "uri",
                    "description": "Sale to delete by ID",
                    "required": true
                }
            },
            "curl.options": {
                "CURLOPT_TIMEOUT": "100"
            }
        },
        "legacy/webservice/sale": {
            "class": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Request_SalesLegacyItemPaid",
            "extends": "base",
            "uri": "/ws1/sale",
            "summary": "Send a notification to tell legacy about a sold item",
            "responseClass": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Response_SalesLegacyItemPaid",
            "parameters": {
                "order_id": {
                    "location": "postField",
                    "type": "integer",
                    "required": true
                },
                "order_item_id": {
                    "location": "postField",
                    "type": "integer",
                    "required": true
                },
                "sku": {
                    "location": "postField",
                    "type": "string",
                    "required": true
                },
                "seller_user_id": {
                    "location": "postField",
                    "type": "integer",
                    "required": true
                },
                "user_art_id": {
                    "location": "postField",
                    "type": "integer",
                    "required": true
                },
                "quantity": {
                    "location": "postField",
                    "type": "integer",
                    "required": true
                },
                "impression": {
                    "location": "postField",
                    "type": "integer",
                    "required": false
                },
                "timestamp": {
                    "location": "postField",
                    "type": "string",
                    "required": true
                },
                "art_unit_price": {
                    "location": "postField",
                    "type": "string"
                },
                "art_unit_cost": {
                    "location": "postField",
                    "type": "string"
                },
                "discount": {
                    "type": "array",
                    "location": "postField"
                }
            },
            "curl.options": {
                "CURLOPT_TIMEOUT": "100"
            }
        },
        "legacy/webservice/header": {
            "class": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Request_LegacyHeader",
            "extends": "base",
            "uri": "/ws1/header",
            "summary": "get the legacy header json",
            "responseClass": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Response_Header",
            "parameters": {}
        },
        "customer/legacy/profile": {
            "class": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Request_CustomerLegacyProfile",
            "extends": "base",
            "uri": "/ws1/user/{user_id}",
            "summary": "Reads a customers profile",
            "responseClass": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Response_CustomerLegacyProfile",
            "httpMethod": "GET",
            "parameters": {
                "user_id": {
                    "location": "uri",
                    "type": "string",
                    "required": true
                }
            },
            "curl.options": {
                "CURLOPT_TIMEOUT": "100"
            }
        },
        "legacy/webservice/blockartist": {
            "class": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Request_BlockArtist",
            "extends": "base",
            "uri": "/ws1/block-artist",
            "summary": "Block a artist",
            "responseClass": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Response_BlockArtist",
            "httpMethod": "POST",
            "parameters": {
                "user_id": {
                    "location": "postField",
                    "type": "integer",
                    "required": true
                }
            },
            "curl.options": {
                "CURLOPT_TIMEOUT": "100"
            }
        },
        "legacy/webservice/blockartwork": {
            "class": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Request_BlockArtwork",
            "extends": "base",
            "uri": "/ws1/block-artwork",
            "summary": "Block a artwork",
            "responseClass": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Response_BlockArtwork",
            "httpMethod": "POST",
            "parameters": {
                "sku": {
                    "location": "postField",
                    "type": "string",
                    "required": true
                },
                "availability": {
                    "location": "postField",
                    "type": "integer",
                    "required": true
                }
            },
            "curl.options": {
                "CURLOPT_TIMEOUT": "100"
            }
        },
        "customer/legacy/facebooklogin": {
            "class": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Request_CustomerLegacyFacebookLogin",
            "extends": "base",
            "uri": "/account/login/facebook-login",
            "summary": "Login a customer with facebook",
            "responseClass": "Sao_Shared_Library_Legacy_Guzzle_Service_Command_Response_CustomerLegacyFacebookLogin",
            "httpMethod": "POST",
            "parameters": {
                "code": {
                    "location": "postField",
                    "type": "string",
                    "required": true
                },
                "redirectUri": {
                    "location": "postField",
                    "type": "string",
                    "required": true
                }
            }
        }
    }
}
